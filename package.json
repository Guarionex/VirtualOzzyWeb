{
  "name": "virtual-ozzy-web",
  "version": "1.0.0",
  "description": "Virtual Ozzy Website",
  "author": "Virtual Ozzy LLC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Guarionex/VirtualOzzyWeb.git"
  },
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": ">=12.2.0",
    "yarn": ">=1.0.0"
  },
  "private": true,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "next build ./client",
    "coverage": "jest --coverage",
    "dev": "cross-env NODE_ENV=local-dev DEV_SERVER=true nodemon --experimental-modules  --es-module-specifier-resolution=node --watch server server/index.mjs",
    "lint": "eslint . --ext .json --ext .js --ext .mjs",
    "test": "yarn lint && yarn coverage",
    "unit": "cross-env NODE_ENV=test jest"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.0",
    "@hapi/catbox-memory": "^5.0.0",
    "@hapi/hapi": "^19.2.0",
    "@hapi/topo": "^5.0.0",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@zeit/next-sass": "^1.0.1",
    "chokidar": "^3.5.1",
    "cleave.js": "^1.6.0",
    "config": "^3.3.1",
    "create-react-class": "^15.6.3",
    "cross-env": "^7.0.2",
    "cypress": "^7.1.0",
    "email-validator": "^2.0.4",
    "formik": "^2.2.6",
    "globby": "^11.0.1",
    "graceful-fs": "^4.2.6",
    "http-status": "^1.4.2",
    "isomorphic-unfetch": "^3.1.0",
    "joi": "^17.4.0",
    "kebab-case": "^1.0.1",
    "lodash": "4.17.19",
    "lodash-pickdeep": "^1.0.2",
    "moment": "^2.27.0",
    "next": "^11.1.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.1",
    "react-text-mask": "^5.4.3",
    "react-transition-group": "^4.4.1",
    "react-virtualized": "^9.21.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "sass": "^1.32.10",
    "sentence-case": "^3.0.3",
    "styled-components": "^5.1.1",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/polyfill": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^12.0.15",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-styled-components": "^1.10.7",
    "chance": "^1.1.6",
    "core-js": "^3.10.1",
    "cypress-cucumber-preprocessor": "^4.0.3",
    "cypress-plugin-retries": "^1.5.2",
    "cypress-promise": "^1.1.0",
    "cypress-xpath": "^1.6.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.5.0",
    "eslint-config-get-off-my-lawn": "^5.0.1",
    "fs-extra": "^9.0.1",
    "jest": "^26.1.0",
    "jest-chain": "^1.1.5",
    "jest-environment-jsdom": "^26.1.0",
    "jest-environment-jsdom-global": "^2.0.4",
    "jest-extended": "^0.11.5",
    "jest-styled-components": "^7.0.2",
    "jest-when": "^2.7.2",
    "nodemon": "^2.0.4",
    "query-string": "^6.13.1",
    "react-addons-test-utils": "^15.6.2",
    "react-select-event": "^5.0.0",
    "react-test-renderer": "^16.13.1",
    "url-parse": "^1.4.7"
  },
  "jest": {
    "cacheDirectory": ".jest-cache",
    "coverageDirectory": ".jest-coverage",
    "coverageReporters": [
      "html",
      "text",
      "lcov"
    ],
    "collectCoverageFrom": [
      "(client|lib|server)/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "projects": [
      {
        "displayName": "server",
        "testEnvironment": "node",
        "testMatch": [
          "<rootDir>/test/server/**/*.spec.mjs",
          "<rootDir>/test/lib/**/*.spec.js"
        ],
        "modileFileExtension": [
          "js",
          "mjs",
          "json"
        ],
        "transform": {
          "^.+\\.mjs$": "babel-jest",
          "^.+\\js$": "babel-jest"
        },
        "moduleNameMapper": {
          "\\.(css|less|scss)$": "<rootDir>/test/mocks/styleMock.js",
          "^@panva/jose$": "<rootDir>/test/mocks/panva-jose-mock.js"
        },
        "coveragePathIgnorePatterns": [
          "<rootDir>/test/"
        ],
        "setupFilesAfterEnv": [
          "<rootDir>/test/test-setup.js"
        ]
      },
      {
        "displayName": "client",
        "testEnvironment": "jest-environment-jsdom-global",
        "testMatch": [
          "<rootDir>/test/client/**/*.spec.js"
        ],
        "testURL": "localhost:8080/path/sub?foo=bar",
        "moduleNameMapper": {
          "\\.(css|less|scss)$": "<rootDir>/test/mocks/styleMock.js",
          "^@panva/jose$": "<rootDir>/test/mocks/panva-jose-mock.js"
        },
        "transform": {
          "^.+\\.js$": "babel-jest"
        },
        "moduleFileExtension": [
          "js",
          "mjs",
          "json"
        ],
        "coveragePathIgnorePatterns": [
          "<rootDir>/test/test-setup.js"
        ]
      }
    ]
  },
  "resolutions": {
    "graceful-fs": "^4.1.15"
  }
}
